  },
  "homepage": "https://github.com/nodeswork/utils#readme",
  "devDependencies": {
    "@types/node": "^8.0.8",
    "chai": "^4.0.2",
    "coffeescript": "^2.0.0-beta3",
    "gulp": "^3.9.1",
    "gulp-typedoc": "^2.0.2",
    "gulp-typescript": "^3.2.1",
    "mocha": "^3.4.2",
    "should": "^11.2.1",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "@types/object-hash": "^1.1.0",
